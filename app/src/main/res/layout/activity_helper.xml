<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/container2"    android:layout_width="match_parent"    android:layout_height="match_parent">        <FrameLayout            android:id="@+id/frameHelper"            android:layout_width="match_parent"            android:layout_height="0dp"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintEnd_toEndOf="parent"></FrameLayout>    <View        android:id="@+id/backHelperBlur"        android:visibility="invisible"        android:background="#CCFFFFFF"        android:layout_width="match_parent"        android:layout_height="match_parent"/>        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <com.google.android.material.floatingactionbutton.FloatingActionButton                android:id="@+id/btnHelperNewCard"                android:layout_width="58dp"                android:layout_height="58dp"                android:layout_marginBottom="6dp"                android:clickable="true"                android:visibility="invisible"                android:src="@drawable/btn_home_download"                app:layout_constraintStart_toStartOf="@id/btnHelperAddCard"                app:layout_constraintEnd_toEndOf="@+id/btnHelperAddCard"                app:layout_constraintBottom_toTopOf="@id/btnHelperAddCard" />            <com.google.android.material.floatingactionbutton.FloatingActionButton                android:id="@+id/btnHelperDownCard"                android:layout_width="58dp"                android:layout_height="58dp"                android:layout_marginBottom="6dp"                android:visibility="invisible"                android:clickable="true"                android:src="@drawable/btn_home_newcard"                app:layout_constraintStart_toStartOf="@id/btnHelperAddCard"                app:layout_constraintEnd_toEndOf="@+id/btnHelperAddCard"                app:layout_constraintBottom_toTopOf="@id/btnHelperNewCard"/>        <com.google.android.material.floatingactionbutton.FloatingActionButton            android:id="@+id/btnHelperAddCard"            android:layout_width="58dp"            android:layout_height="58dp"            android:visibility="visible"            android:src="@drawable/btn_tabbar_plus"            android:clickable="true"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintBottom_toBottomOf="parent"            android:layout_marginBottom="21dp" />        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/cstHelperBottom"            android:layout_width="413dp"            android:layout_height="49dp"            android:background="#ffffff"            android:outlineSpotShadowColor="@color/black_10"            android:visibility="visible"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent">                <ImageView                    android:id="@+id/btnHelperGoDisabled"                    android:layout_width="30dp"                    android:layout_height="30dp"                    android:src="@drawable/ic_star_black_24dp"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toStartOf="@id/btnHelperAllCard"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toTopOf="parent" />                <ImageView                    android:id="@+id/btnHelperAllCard"                    android:layout_width="50dp"                    android:layout_height="match_parent"                    android:layout_weight="1"                    android:src="@drawable/ic_star_half_black_24dp"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toEndOf="parent"                    app:layout_constraintStart_toEndOf="@id/btnHelperGoDisabled"                    app:layout_constraintTop_toTopOf="parent"                    app:layout_constraintVertical_bias="0.0" />        </androidx.constraintlayout.widget.ConstraintLayout>    </androidx.constraintlayout.widget.ConstraintLayout></androidx.constraintlayout.widget.ConstraintLayout>